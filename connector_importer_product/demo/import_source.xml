<odoo>

  <record id="demo_import_source_csv_product_category" model="import.source.csv">
    <field name="name">product_category</field>
    <field name="chunk_size" eval="0" />  <!-- do not chunk CSV lines -->
    <field name="csv_delimiter">,</field>
    <field name="csv_encoding">utf-8</field>
  </record>

  <record id="demo_import_source_csv_product_attribute" model="import.source.csv">
    <field name="name">product_attribute</field>
    <field name="chunk_size" eval="0" />  <!-- do not chunk CSV lines -->
    <field name="csv_delimiter">,</field>
    <field name="csv_encoding">utf-8</field>
  </record>

  <record id="demo_import_source_csv_product_attribute_value" model="import.source.csv">
    <field name="name">product_attribute_value</field>
    <field name="chunk_size" eval="0" />  <!-- do not chunk CSV lines -->
    <field name="csv_delimiter">,</field>
    <field name="csv_encoding">utf-8</field>
  </record>

  <record id="demo_import_source_csv_product_product" model="import.source.csv">
    <field name="name">product_product</field>
    <field name="chunk_size" eval="300" />
    <field name="csv_delimiter">,</field>
    <field name="csv_encoding">utf-8</field>
  </record>

  <record id="demo_import_source_csv_product_supplierinfo" model="import.source.csv">
    <field name="name">product_supplierinfo</field>
    <field name="chunk_size" eval="0" />  <!-- do not chunk CSV lines -->
    <field name="csv_delimiter">,</field>
    <field name="csv_encoding">utf-8</field>
  </record>

  <record id="demo_import_source_csv_product_packaging" model="import.source.csv">
    <field name="name">product_packaging</field>
    <field name="chunk_size" eval="0" />  <!-- do not chunk CSV lines -->
    <field name="csv_delimiter">,</field>
    <field name="csv_encoding">utf-8</field>
  </record>

</odoo>
