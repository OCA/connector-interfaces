<odoo>

  <record id="demo_import_type_product_category" model="import.type">
    <field name="name">Import Product Category</field>
    <field name="key">product_category</field>
    <field name="options">
- model: product.category
  importer: product.category.importer
    </field>
  </record>

  <record id="demo_import_type_product_attribute" model="import.type">
    <field name="name">Import Product Attribute</field>
    <field name="key">product_attribute</field>
    <field name="options">
- model: product.attribute
  importer: product.attribute.importer
    </field>
  </record>

  <record id="demo_import_type_product_attribute_value" model="import.type">
    <field name="name">Import Product Attribute Value</field>
    <field name="key">product_attribute_value</field>
    <field name="options">
- model: product.attribute.value
  importer: product.attribute.value.importer
  options:
    importer:
      break_on_error: true
    </field>
  </record>

  <record id="demo_import_type_product_product" model="import.type">
    <field name="name">Import Product</field>
    <field name="key">product_product</field>
    <field name="options">
- model: product.product
  importer: product.product.importer
    </field>
  </record>

  <record id="demo_import_type_product_supplierinfo" model="import.type">
    <field name="name">Import Product Supplier Info</field>
    <field name="key">product_supplierinfo</field>
    <field name="options">
- model: product.supplierinfo
  importer: product.supplierinfo.importer
    </field>
  </record>

  <record id="demo_import_type_product_packaging" model="import.type">
    <field name="name">Import Product Packaging</field>
    <field name="key">product_packaging</field>
    <field name="options">
- model: product.packaging
  importer: product.packaging.importer
    </field>
  </record>

</odoo>
